#include <stdio.h>
#include <stdlib.h>

int main(){
    
    int num = 1, anterior = 0, soma = 1;
    
    printf("SEQUÊNCIA DE FIBONACCI ATÉ O 20º TERMO!!!\n\n");
    printf("{ ");
    for(int i = 0; i < 20; i++){
        printf("%d ", soma);
        soma = num + anterior;
        
        anterior = num;
        num = soma;
    }
    
    printf("}");
    
    return 0;
}
